{
    "name": "malaya-missions-project",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "dev": "vite dev",
        "vercel-dev": "vercel dev --token $VERCEL_TOKEN --yes",
        "build": "vite build",
        "preview": "vite preview",
        "test": "playwright test",
        "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
        "test:unit": "concurrently \"npm run vercel-dev\" \"vitest --run\" --kill-others -s first",
        "test:watch": "concurrently \"npm run vercel-dev\" \"vitest\" --kill-others",
        "codegen": "concurrently \"npm run vercel-dev\" \"playwright codegen\" --kill-others",
        "test:ui": "concurrently \"npm run vercel-dev\" \"playwright test --ui\" --kill-others",
        "test:coverage": "concurrently \"npm run vercel-dev\" \"vitest run --coverage\" --kill-others -s first",
        "test:results": "npx playwright show-report e2e/test-results",
        "lint": "prettier --plugin-search-dir . --check . && eslint .",
        "lintfix": "prettier --plugin-search-dir . --write . && eslint .",
        "format": "prettier --plugin-search-dir . --write .",
        "prepare": "husky install",
        "prisma-push": "./prisma/apply_scripts.sh"
    },
    "lint-staged": {
        "*.{js,ts,svelte}": "eslint",
        "*.{js,ts,svelte,css,scss,postcss,md,json}": [
            "prettier --write --plugin-search-dir=.",
            "prettier --check --plugin-search-dir=."
        ]
    },
    "prisma": {
        "seed": "node --experimental-specifier-resolution=node --loader ts-node/esm prisma/seed.ts"
    },
    "devDependencies": {
        "@commitlint/cli": "^17.6.6",
        "@commitlint/config-conventional": "^17.6.7",
        "@faker-js/faker": "^8.0.2",
        "@floating-ui/dom": "^1.4.5",
        "@fortawesome/free-brands-svg-icons": "^6.4.0",
        "@fortawesome/free-regular-svg-icons": "^6.4.0",
        "@fortawesome/free-solid-svg-icons": "^6.4.0",
        "@planetscale/database": "^1.8.0",
        "@playwright/test": "^1.36.1",
        "@prisma/client": "^5.0.0",
        "@skeletonlabs/skeleton": "^1.10.0",
        "@sveltejs/adapter-auto": "^2.0.0",
        "@sveltejs/adapter-vercel": "^3.0.2",
        "@sveltejs/kit": "^1.20.4",
        "@tailwindcss/forms": "^0.5.4",
        "@tailwindcss/typography": "^0.5.9",
        "@testing-library/jest-dom": "^5.17.0",
        "@testing-library/svelte": "^4.0.3",
        "@types/bcryptjs": "^2.4.2",
        "@types/node": "^20.4.2",
        "@types/testing-library__jest-dom": "^5.14.8",
        "@types/uuid": "^9.0.2",
        "@typescript-eslint/eslint-plugin": "^5.62.0",
        "@typescript-eslint/parser": "^5.62.0",
        "@vercel/node": "^2.15.6",
        "@vitest/coverage-c8": "^0.33.0",
        "autoprefixer": "^10.4.14",
        "bcryptjs": "^2.4.3",
        "concurrently": "^8.2.0",
        "dotenv": "^16.3.1",
        "eslint": "^8.45.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-plugin-svelte": "^2.32.2",
        "husky": "^8.0.3",
        "jsdom": "^22.1.0",
        "lint-staged": "^13.2.3",
        "module-alias": "^2.2.3",
        "postcss": "^8.4.26",
        "prettier": "^2.8.8",
        "prettier-plugin-svelte": "^2.10.1",
        "prisma": "^5.0.0",
        "svelte": "^4.0.5",
        "svelte-check": "^3.4.6",
        "svelte-fa": "^3.0.4",
        "svelte-forms-lib": "^2.0.1",
        "tailwindcss": "^3.3.3",
        "ts-interface-checker": "^1.0.2",
        "ts-node": "^10.9.1",
        "tslib": "^2.6.0",
        "typescript": "^5.1.6",
        "uuid": "^9.0.0",
        "vercel": "^31.0.4",
        "vite": "^4.4.4",
        "vitest": "^0.32.4",
        "yup": "^1.2.0"
    },
    "type": "module"
}
